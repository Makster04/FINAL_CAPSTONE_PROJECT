    "import pandas as pd\n",
    "\n",
    "# Reload all updated uploaded files\n",
    "paths = {\n",
    "    \"1980\": \"Pre_1980_Recession.csv\",\n",
    "    \"1981\": \"Pre_1981_Recession.csv\",\n",
    "    \"1990\": \"Pre_1990_Recession.csv\",\n",
    "    \"2001\": \"Pre_2001_Recession.csv\",\n",
    "    \"2008\": \"Pre_2008_Recession.csv\",\n",
    "    \"2020\": \"Pre_2020_Recession.csv\"\n",
    "}\n",
    "\n",
    "# Read and combine data\n",
    "recession_dfs = []\n",
    "for label, path in paths.items():\n",
    "    df = pd.read_csv(path, parse_dates=[\"Date\"])\n",
    "    df[\"Recession_Period\"] = label\n",
    "    recession_dfs.append(df)\n",
    "\n",
    "# Combine into one DataFrame\n",
    "combined_df = pd.concat(recession_dfs, ignore_index=True)\n",
    "combined_df.sort_values(\"Date\", inplace=True)\n",
    "\n",
    "# Aggregate averages for selected indicators by Recession_Period\n",
    "summary = combined_df.groupby(\"Recession_Period\").agg({\n",
    "    'Fed_Funds_Rate_Change': 'mean',\n",
    "    'Yield_Curve_Change': 'mean',\n",
    "    'Business_Sentiment_Growth_Rate': 'mean',\n",
    "    'Consumer_Sentiment_Growth_Rate': 'mean',\n",
    "    'Housing_Starts_Change': 'mean',\n",
    "    'Jobless_Claims_Growth_Rate': 'mean',\n",
    "    'Job_Growth_Rate': 'mean',\n",
    "    'GDP_Growth_Rate': 'mean',\n",
    "    'Change_in_CPI': 'mean',\n",
    "    'Change_in_PPI': 'mean',\n",
    "    'Unemployment_Rate_Change': 'mean',\n",
    "    'Industrial_Production_Growth_Rate': 'mean',\n",
    "    'Capacity_Utilization_Growth_Rate': 'mean'\n",
    "}).round(2)\n",
    "\n",
    "summary.reset_index(inplace=True)\n",
    "\n",
    "# Format as Markdown table\n",
    "summary"
   ]
